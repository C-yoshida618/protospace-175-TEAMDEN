<main class="main">
  <div class="inner">
    <% if user_signed_in? %>
      <div class="greeting">
        <%= "こんにちは" %>
        <% if @prototype.present? %>
          <%= link_to "#{current_user.name}さん", user_path(@prototype.user), class: :greeting__link %>
        <% end %>
      </div>
    <% else %>
      <div class="grid-6">
        <%= link_to "", new_user_session_path, class: "post" %>
        <%= link_to "", new_user_registration_path, class: "post" %>
      </div>
    <% end %>
  </div>
  <div class="card__wrapper">
    <%= render partial: "prototype", collection: @prototypes %>
  </div>
</main>
