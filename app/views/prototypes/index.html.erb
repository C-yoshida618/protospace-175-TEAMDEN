<main class="main">
  <div class="inner">
    <% if user_signed_in? && current_user.id == prototype.user_id %>
      <div class="greeting">
        <%= "こんにちは #{current_user.name}さん" %>
      </div>
    <% else %>
      <div class="grid-6">
        <%= link_to "", new_user_session_path, class: "post" %>
        <%= link_to "", new_user_registration_path, class: "post" %>
      </div>
    <% end %>
  </div>

  <div class="card__wrapper">
    <%= render partial: "prototypes/prototype", collection: @prototypes %>
  </div>
</main>